public class HidratoCarbono extends Alimento {
    public HidratoCarbono(String nombre, double calorias, double gramosPorPorcion) {
        super(nombre, calorias, gramosPorPorcion);
    }
    @Override
    public void mostrarInformacion() {
        System.out.println("Hidrato de Carbono: " + Nombre + ", Calor√≠as: " + Calorias + ", gramos: " + Gramos_Por_Porcion);
    }

    @Override
    public String getCategoria() {
        return "Hidratos de Carbono";
    }
}
