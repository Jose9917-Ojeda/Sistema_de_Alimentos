public class Grasa extends Alimento {
    public Grasa(String nombre, double calorias, double gramosPorPorcion) {
        super(nombre, calorias, gramosPorPorcion);
    }

    @Override
    public void mostrarInformacion() {
        System.out.println("Grasa: " + Nombre + ", Calor√≠as: " + Calorias + ", gramos: " + Gramos_Por_Porcion);
    }

    @Override
    public String getCategoria() {
        return "Grasas";
    }
}
